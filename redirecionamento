        <script type="text/javascript">
        var contador = 0;
        function contar()
        {

            document.getElementById('contador').innerHTML = contador;
            if (contador < 10)
           {
               contador++
               document.getElementById('cont').style.width = contador + "%";
           }
       }
       function redirecionar()
       {
           contar();
           if (contador == 10)
           {
               document.location.href = 'https://api.whatsapp.com/send?phone=5521992730105&text=Ol%C3%A1%2C%20Gostaria%20de%20solicitar%20a%20tabela%20de%20pre%C3%A7os';
           }
       }

       setInterval(redirecionar, 1000);
       setInterval(contar, 10);
